<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="ArmorSmithableBase">
        <defName>COG_StandardHeavyArmor</defName>
        <label>standard heavy armor</label>
        <description>Placeholder</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/PlateArmor/PlateArmor</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <recipeMaker>
            <researchPrerequisite/>
			<unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>11</Crafting>
			</skillRequirements>
            <recipeUsers>
                <li>ElectricSmithy</li>
                <li>FueledSmithy</li>
            </recipeUsers>
		</recipeMaker>
        <costList>
            <Plasteel>60</Plasteel>
            <Uranium>15</Uranium>
            <ComponentIndustrial>5</ComponentIndustrial>
        </costList>
        <statBases>
            <WorkToMake>38000</WorkToMake>
            <MaxHitPoints>1000</MaxHitPoints>
            <Mass>20</Mass>
            <Flammability>0</Flammability>
            <MarketValue>2100</MarketValue>
            <ArmorRating_Sharp>0.9</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.45</ArmorRating_Blunt>
            <ArmorRating_Heat>0.65</ArmorRating_Heat>
            <Insulation_Cold>4</Insulation_Cold>
            <Insulation_Heat>0</Insulation_Heat>
            <EquipDelay>20</EquipDelay>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
            <li>RewardStandardQualitySuper</li>
        </thingSetMakerTags>
        <equippedStatOffsets>
            <MoveSpeed>-0.5</MoveSpeed>
            <MeleeDodgeChance>-5</MeleeDodgeChance>
        </equippedStatOffsets>
        <thingCategories>
            <li>ApparelArmor</li>
        </thingCategories>
        <apparel>
            <bodyPartGroups>
                <li>Torso</li>
                <li>Neck</li>
                <li>Shoulders</li>
                <li>Arms</li>
                <li>Legs</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/PlateArmor/PlateArmor</wornGraphicPath>
            <layers>
                <li>Middle</li>
                <li>Shell</li>
            </layers>
            <tags Inherit="False">
                <li>Apparel_Scavenger</li>
                <li>Apparel_ScavengerMilitary</li>
                <li>ScavHeavyApparel</li>
            </tags>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
        </apparel>
    </ThingDef>

