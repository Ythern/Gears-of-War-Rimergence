<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="ArmorHelmetMakeableBase">
        <defName>COG_StandardHelmet</defName>
        <label>standard helmet</label>
        <description>Placeholder.</description>
        <graphicData>
            <texPath>Things/Pawn/Humanlike/Apparel/ReconArmorHelmet/ReconHelmet</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <costList>
            <Plasteel>35</Plasteel>
            <Uranium>15</Uranium>
            <ComponentIndustrial>5</ComponentIndustrial>
        </costList>
        <recipeMaker>
            <researchPrerequisite/>
            <skillRequirements>
				<Crafting>11</Crafting>
			</skillRequirements>
            <recipeUsers>
              <li>ElectricSmithy</li>
              <li>FueledSmithy</li>
            </recipeUsers>
        </recipeMaker>
        <statBases>
            <WorkToMake>14000</WorkToMake>
            <MaxHitPoints>250</MaxHitPoints>
            <MarketValue>450</MarketValue>
            <Mass>5</Mass>
            <ArmorRating_Sharp>0.9</ArmorRating_Sharp>
            <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
            <ArmorRating_Heat>0.60</ArmorRating_Heat>
            <Insulation_Cold>0</Insulation_Cold>
            <Insulation_Heat>3</Insulation_Heat>
            <EquipDelay>6</EquipDelay>
        </statBases>
        <thingSetMakerTags>
            <li>RewardStandardLowFreq</li>
            <li>RewardStandardQualitySuper</li>
        </thingSetMakerTags>
        <equippedStatOffsets>
            <MoveSpeed>-0.2</MoveSpeed>
            <ShootingAccuracyPawn>-0.5</ShootingAccuracyPawn>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>
                <li>FullHead</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Pawn/Humanlike/Apparel/ReconArmorHelmet/ReconHelmet</wornGraphicPath>
            <layers>
                <li>Overhead</li>
            </layers>
            <tags Inherit="False">
                <li>Apparel_Scavenger</li>
                <li>Apparel_ScavengerMilitary</li>
                <li>ScavHeavyApparel</li>
            </tags>
            <defaultOutfitTags>
                <li>Soldier</li>
            </defaultOutfitTags>
            <hatRenderedAboveBody>false</hatRenderedAboveBody>
        </apparel>
    </ThingDef>

